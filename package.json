{
  "name": "msec-academics",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm exec vite build && echo 'Build completed successfully'",
    "preview": "npm exec vite preview",
    "server": "node server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "warm-tunnel": "node scripts/warm-tunnel.js",
    "dev:warm": "concurrently \"npm run server\" \"npm run dev\" \"npm run warm-tunnel\"",
    "clean": "rmdir /s /q dist 2>nul || echo \"Directory already clean\"",
    "prebuild": "npm run clean",
    "seed-users": "node seed-users.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "lru-cache": "^11.2.2",
    "lucide-react": "^0.553.0",
    "moment": "^2.30.1",
    "mongoose": "^8.18.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "pdfkit": "^0.15.2",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "sharp": "^0.34.5",
    "twilio": "^4.19.0",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.22",
    "concurrently": "^7.6.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vite": "^4.1.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.0.4"
  }
}
